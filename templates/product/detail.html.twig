{% extends "base.html.twig" %}

{% block body %}
    <h1>liste de produits</h1>
    <ul>
        {% for key, product in array %}
            {#  <li>{{ key }} {{ value }}</li>#}

            <li>{{ key | upper ~ '/' ~ product }}</li>
        {% endfor %}
    </ul>
    <p>
        Date: {{ now | date('d/m/y') }}
    </p>
{% endblock %}
